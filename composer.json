{
    "name": "opis/events",
    "description": "Opis Events is a library that can be used for dispatching and intercepting events. This library is built on top of the Opis Routing library and provides a full range of features like filters and events' priorities.",
    "keywords": ["events", "dispatch", "event", "handler"],
    "homepage": "http://opis.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "opis/routing": "5.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "Opis\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\Events\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    }
}
