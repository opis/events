{
    "name": "opis/events",
    "description": "Opis Events is a library that can be used for dispatching and intercepting events",
    "keywords": ["events", "dispatch", "event", "handler"],
    "homepage": "https://opis.io/events",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        },
        {
            "name": "Sorin Sarca",
            "email": "sarca_sorin@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "opis/utils": "2020.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "opis/closure": "4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Opis\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\Events\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2020.x-dev"
        }
    }
}
